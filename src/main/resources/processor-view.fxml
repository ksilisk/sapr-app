<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" prefHeight="300" prefWidth="400" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ksilisk.sapr.controller.ProcessorController">
    <TextArea minHeight="200" fx:id="result" editable="false" focusTraversable="false"
              promptText="Нажмите кнопку рассчитать, чтобы полуить результаты"/>
    <Button minHeight="50" minWidth="200" onMouseClicked="#click" text="Рассчитать"/>
</VBox>
